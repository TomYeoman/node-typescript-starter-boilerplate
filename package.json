{
  "name": "node-with-linting",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "// ------ DEVELOPMENT ----- //": "",
    "debug": "nodemon src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "// ------ TESTING ----- //": "",
    "test": "jest --watch",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "// ------ BUILDING ----- //": "",
    "build": "tsc",
    "build:watch": "tsc -w",
    "// ------ LINTING ----- //": "",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "// ------ PRODUCTION ----- //": "",
    "start:prod": "nodemon src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.10.5",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.3.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}

